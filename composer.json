{
    "name": "laravesl/mailer-sdk",
    "description": "Mailer SDK: Helper, Library and Quick Service - Basic, Simple and Lightweight",
    "type": "library",
    "license": "BSD-3-Clause",
    "version":"1.0.5",
    "require": {
        "jackiedo/dotenv-editor": "*",
        "php": "^8.2.0"
    },
    "autoload": {
        "psr-4": {
            "Laravesl\\MailerSdk\\": "src/"
        },
        "files": [
            "src/mPH.php"
        ]
    },
    "authors": [
        {
            "name": "laravesl, xMailerSDK"
        }
    ],
    "extra": {
        "laravel": {
